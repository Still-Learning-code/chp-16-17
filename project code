 fun String.greet(): String {
     return "Hello $this"
 }
 fun Int.isEven(): Boolean {
     return this % 2 == 0
 }
 fun main(){  //Chp 17 Extension Functions (.) dot used to call extension functions,access extension properties
   println("Kotlin".greet())
     println(1.isEven())
     println(2.isEven())
 }
